{
  "name": "ai-layout-optimizer",
  "version": "1.0.0",
  "description": "Standalone AI-powered layout optimizer library for Airport City mobile game",
  "module": "index.ts",
  "type": "module",
  "main": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./types": "./types.ts"
  },
  "scripts": {
