{
  "name": "ai-layout-optimizer",
  "version": "1.0.0",
  "description": "Standalone AI-powered layout optimizer library for Airport City mobile game",
  "module": "index.ts",
  "type": "module",
  "main": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./types": "./types.ts"
  },
  "scripts": {
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "build": "bun build index.ts --outdir ./dist",
    "example": "bun run example.ts",
    "dev": "bun run ./src/index.ts --watch",
    "start": "bun run ./src/api/server.ts",
    "api:dev": "bun run ./src/api/server.ts --watch"
  },
  "keywords": [
    "airport-city",
    "layout",
    "optimizer",
    "ai",
    "game"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.17"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@google/genai": "^1.8.0",
    "@prisma/client": "^6.11.1",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "express": "^4.18.2",
    "prisma": "^6.11.1"
  }
}
